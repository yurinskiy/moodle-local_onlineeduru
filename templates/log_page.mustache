{{!
    @template local_onlineeduru/log
}}
<div data-region="log">
    <dl>
        <dt>URL</dt>
        <dd>{{log.url}}</dd>

        <dt>Метод</dt>
        <dd>{{log.method}}</dd>

        <dt>Статус</dt>
        <dd>{{log.status}}</dd>

        <dt>Время</dt>
        <dd>{{log.timecreated}}</dd>

        <dt>Текст запроса</dt>
        <dd>
            <pre>{{request}}</pre>
        </dd>

        <dt>Текст ответа</dt>
        <dd>
            <pre>{{response}}</pre>
        </dd>
    </dl>

</div>